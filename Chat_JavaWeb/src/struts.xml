<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.devMode" value="true"></constant>
	<constant name="struts.configuration.xml.reload" value="true"></constant>
	<constant name="struts.i18n.encoding" value="UTF-8"></constant>
	<constant name="struts.multipart.saveDir" value="/tmp"></constant>
	<package name="defalut" namespace="/" extends="struts-default">
		<action name="register" class="com.lz.action.RegisterAction"
			method="execute">
			<result name="success">./registersuccess.jsp</result>
		</action>

		<action name="update" class="com.lz.action.UpdateAction"
			method="execute">
			<result name="success">./updateinfo.jsp</result>
		</action>
	</package>
</struts>    
